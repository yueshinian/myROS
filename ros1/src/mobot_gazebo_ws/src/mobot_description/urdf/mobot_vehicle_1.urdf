      <leftFrontJoint>joint_wheel_lf</leftFrontJoint>
      <rightFrontJoint>joint_wheel_rf</rightFrontJoint>
      <leftRearJoint>joint_wheel_lb</leftRearJoint>
      <rightRearJoint>joint_wheel_rb</rightRearJoint>
      <wheelSeparation>0.7802</wheelSeparation>
      <wheelDiameter>0.350</wheelDiameter>
      <robotBaseFrame>base_footprint</robotBaseFrame>
      <odometryFrame>odom</odometryFrame>
      <torque>50</torque>
      <commandTopic>navigation_command</commandTopic>
      <broadcastTF>1</broadcastTF>
      <odometryTopic>odom</odometryTopic>
    </plugin>
  </gazebo>
